<?xml version="1.0" encoding="UTF-8"?>
<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
<function name="kw-align" operations="kw-attribute,kw-browser" inputPage="xTest.home" mode="module" threshold="0" active="true">
<description>Test the alignment of elements on the screen</description>
<parameter type="STATIC" name="alignOne" value=""/>
<step name="toggleButton" site="xTest" page="home" type="ALIGN" active="true" threshold="0" wait="0" failureMode="ERROR">
<parameter type="STATIC" name="null" value="LEFT=toggleValue"/>
</step>
<step name="confirmValue" site="xTest" page="home" type="ALIGN" active="true" threshold="0" wait="0" failureMode="ERROR">
<parameter type="STATIC" name="null" value="RIGHT=promptValue"/>
</step>
<step name="confirmValue" site="xTest" page="home" type="ALIGN" active="true" threshold="0" wait="0" failureMode="ERROR">
<parameter type="STATIC" name="null" value="RIGHT=promptValue(50)"/>
</step>
<step name="alertButton" site="xTest" page="home" type="ALIGN" active="true" threshold="0" wait="0" failureMode="ERROR">
<parameter type="STATIC" name="null" value="TOP=confirmButton"/>
<parameter type="STATIC" name="null" value="BOTTOM=confirmButton"/>
</step>
</function>
</suite>
