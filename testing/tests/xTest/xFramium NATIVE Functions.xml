  <test name="SQL Test" tagNames="Native" timed="true" threshold="0" active="true" priority="1" severity="1">
    <description>Test the Perfecto NATIVE Notifications Commands</description>
    <step type="RANDOM" name="FIRST_NAME" page="home" context="USERID" />
    <step name="buildTable1" page="Register" type="SQL">
      <parameter type="STATIC" value="create table table1 (column1 varchar(2), column2 varchar(2), column3 varchar(2), column4 varchar(2), column5 varchar(2))" />
    </step>
    <step name="buildTable2" page="Register" type="SQL">
      <parameter type="STATIC" value="create table table2 (column1 varchar(2), column2 varchar(2), column3 varchar(2), column4 varchar(2), column5 varchar(2))" />
      <parameter name="connectionName" type="STATIC" value="xxx" />
    </step>
    <step name="getKeyTextGuidProcess" page="Register" type="SQL" context="guidresults">
      <parameter type="STATIC" value="SELECT CK.column1 as KEY_TXT ,OP.column2 as USR_ID  FROM Table1 CK INNER JOIN Table2  OP ON CK.column3 = OP.column4 AND OP.column2 = ?  WHERE CK.column5 !='Y' " />
      <parameter type="CONTEXT" value="USERID" />
    </step>
  </test>
