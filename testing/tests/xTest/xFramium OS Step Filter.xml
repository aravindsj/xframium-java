<?xml version="1.0" encoding="UTF-8"?>
<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
<test name="xFramium OS Step Filter" dataProvider="callTest" threshold="0" active="true" os="WINDOWS">
<description>Tests that the specified step is ignored for a specific OS type</description>
<step name="toggleButton" site="xTest" page="home" type="EXISTS" active="true" threshold="0" wait="0" failureMode="ERROR"/>
<step name="toggleValue" site="xTest" page="home" type="EXISTS" active="true" threshold="0" wait="0" failureMode="ERROR" context="existsSingle"/>
<step name="existsCompare" site="xTest" page="home" type="COMPARE" active="true" threshold="0" wait="0" failureMode="ERROR">
<parameter type="STATIC" name="null" value="1"/>
<parameter type="CONTEXT" name="null" value="existsSingle"/>
</step>
<step name="toggleButton" site="xTest" page="home" type="CLICK" active="true" os="ANDROID,IOS" threshold="0" wait="0" failureMode="ERROR"/>
<step name="existsCompare" site="xTest" page="home" type="COMPARE" active="true" threshold="0" wait="0" failureMode="ERROR">
<parameter type="STATIC" name="null" value="1"/>
<parameter type="CONTEXT" name="null" value="existsSingle"/>
</step>
</test>
</suite>
