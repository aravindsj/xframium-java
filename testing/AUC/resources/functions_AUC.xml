<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">

	
		
	<!--########### Functions ############  -->	
	
	<function name="testSetup" active="true">
	<description>
		*******************************************************************
		** Function Name: testSetup
		** Function Desc: Opens the AUC App, if it doesn't exist, it will install and then open
		** Function Params: None
		** Code Sample
		<!-- 
				<step name="testSetup" page="Login" type="CALL2" >
					<parameter type="STATIC" value="" />
				</step>			
		 -->
		** Created By: Andy Dowiarz
		** Change Log: 
		** 			03/20/2017 - adowiarz - New Function
		** NOTES: None 
		*******************************************************************    
	</description>
    <step name="OPEN" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="true" wait="0" failureMode="IGNORE">
      <parameter type="STATIC" name="Application Name" value="AUC_app" />
      <step name="INSTALL" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="false" wait="0">
        <parameter type="STATIC" name="Application Name" value="AUC_app" />
        <parameter type="STATIC" name="Instrument" value="true" />
      </step>
      <step name="OPEN" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="false" wait="0">
        <parameter type="STATIC" name="Application Name" value="AUC_app" />
      </step>
    </step>
    <step name="SEND_NOTIFICATIONS" site="DeVry" page="Home" type="WAIT_FOR" active="true" os="IOS" threshold="0" timed="true" inverse="false" wait="2000" failureMode="IGNORE">
      <step name="SEND_NOTIFICATIONS" site="DeVry" page="Home" type="CLICK" active="true" threshold="0" timed="true" inverse="false" wait="0" failureMode="ERROR" />     
      <step name="OPEN" site="Ross" page="Home" type="APPLICATION" >
        <parameter type="STATIC" name="Application Name" value="AUC_app" />
      </step>      
    </step>
    
	<!--########### SWITCH TO WEBVIEW APP ############  -->
	<step timed="true" name="CONTEXT" page="Home" type="DEVICE">
		<parameter type="STATIC" value="WEBVIEW_1" />
	</step>    
    
    <step name="MENU_LINK.CLICKABLE" site="DeVry" page="Home" type="WAIT_FOR" threshold="30000" failureMode="IGNORE">
		<parameter type="STATIC" value="30" />
		<step name="elseCondition" site="DeVry" page="Home" type="ELSE" failureMode="ERROR">
			<step name="CLOSE" site="Ross" page="Home" type="APPLICATION" wait="10000">
				<parameter type="STATIC" name="Application Name" value="AUC_app" />
			</step> 
			<step name="OPEN" site="Ross" page="Home" type="APPLICATION" wait="10000" >
				<parameter type="STATIC" name="Application Name" value="AUC_app" />
			</step>  
			<step name="MENU_LINK.CLICKABLE" site="DeVry" page="Home" type="WAIT_FOR" failureMode="ERROR" threshold="30000">
				<parameter type="STATIC" value="30" />
			</step>
			<step name="MENU_Second_Time" type="REPORT" page="Home">
				<parameter type="STATIC" value="Test Setup. Found Left Hamburger menu on second try." />
			</step>        
		</step>
		<step name="MENU_First_Time" type="REPORT" page="Home">
			<parameter type="STATIC" value="Test Setup. Found Left Hamburger menu on first try." />
		</step>      
	</step>

	<!--########### SWITCH TO NATIVE APP ############  -->
	<step timed="true" name="CONTEXT" page="Home" type="DEVICE">
		<parameter type="STATIC" value="NATIVE_APP" />
	</step>    
    
  </function>
  
	<function name="testSetup2" active="true">
	<description>
		*******************************************************************
		** Function Name: testSetup
		** Function Desc: Opens the AUC App, if it doesn't exist, it will install and then open
		** Function Params: None
		** Code Sample
		<!-- 
				<step name="testSetup" page="Login" type="CALL2" >
					<parameter type="STATIC" value="" />
				</step>			
		 -->
		** Created By: Andy Dowiarz
		** Change Log: 
		** 			03/20/2017 - adowiarz - New Function
		** NOTES: None 
		*******************************************************************    
	</description>
    <step name="OPEN" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="true" wait="0" failureMode="IGNORE">
      <parameter type="STATIC" name="Application Name" value="AUC_app" />
      <step name="INSTALL" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="false" wait="0">
        <parameter type="STATIC" name="Application Name" value="AUC_app" />
        <parameter type="STATIC" name="Instrument" value="true" />
      </step>
      <step name="OPEN" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="false" wait="0">
        <parameter type="STATIC" name="Application Name" value="AUC_app" />
      </step>
    </step>
    <step name="SEND_NOTIFICATIONS" site="DeVry" page="Home" type="WAIT_FOR" active="true" os="IOS" threshold="0" timed="true" inverse="false" wait="2000" failureMode="IGNORE">
      <step name="SEND_NOTIFICATIONS" site="DeVry" page="Home" type="CLICK" active="true" threshold="0" timed="true" inverse="false" wait="0" failureMode="ERROR" />
      <step name="OPEN_APP" site="DeVry" page="Home" type="DEVICE" active="true" threshold="0" timed="true" inverse="false" wait="2000" failureMode="ERROR">
        <parameter type="STATIC" name="new parameter" value="AUC_app" />
      </step>
    </step>
    <step name="MENU_LINK" site="DeVry" page="Home" type="WAIT_FOR" active="true" threshold="0" timed="true" inverse="false" wait="0" failureMode="IGNORE">
      <step name="elseCondition" site="DeVry" page="Home" type="ELSE" active="true" threshold="0" timed="false" inverse="false" wait="0" failureMode="ERROR">
        <step name="CLOSE_APP" site="DeVry" page="Home" type="DEVICE" active="true" threshold="0" timed="true" inverse="false" wait="5000" failureMode="ERROR">
          <parameter type="STATIC" name="new parameter" value="AUC_app" />
        </step>
        <step name="OPEN_APP" site="DeVry" page="Home" type="DEVICE" active="true" threshold="0" timed="true" inverse="false" wait="5000" failureMode="ERROR">
          <parameter type="STATIC" name="new parameter" value="AUC_app" />
        </step>
        <step name="MENU_LINK" site="DeVry" page="Home" type="EXISTS" active="true" threshold="0" timed="true" inverse="false" wait="0" failureMode="ERROR" />
      </step>
    </step>
  </function>  
  
	<function name="Cleanup" mode="function" active="true">
    <description>
		*******************************************************************
		** Function Name: Cleanup
		** Function Desc: This function will detect a failed run and uninstall the application.
		** Function Params: None
		** Created By: Andy Dowiarz
		** Change Log: 
		** 			3/20/2017 - adowiarz - New Function
		** NOTES: None 
		*******************************************************************      
	</description>
    <step name="STRING" site="DeVry" page="Home" type="COMPARE2" active="true" threshold="0" timed="false" inverse="false" wait="0" failureMode="IGNORE">
      <parameter type="CONTEXT" name="Value One" value="XF_TEST_STATUS" />
      <parameter type="STATIC" name="Value Two" value="FAILED" />
      <parameter type="STATIC" name="Operator" value="EQUALS" />
      <step name="Undefined" site="DeVry" page="Home" type="REPORT" active="true" threshold="0" timed="false" inverse="false" wait="0">
        <parameter type="STATIC" name="Item One" value="Uninstalling after failed run" />
      </step>
      <step name="UNINSTALL" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="false" wait="0">
        <parameter type="STATIC" name="Application Name" value="AUC_app" />
      </step>
    </step>
	</function>		
  
	<function name="deviceSetup" mode="function" active="true">
	<description>
		*******************************************************************
		** Function Name: deviceSetup
		** Function Desc: This function will uninstall and install the application.
		** Function Params: None
		** Created By: Andy Dowiarz
		** Change Log: 
		** 			3/20/2017 - adowiarz - New Function
		** NOTES: None 
		*******************************************************************      
	</description>
		
		<!--#######################  UNINSTALL THE APP ###################-->
		<step name="UNINSTALL" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="0" timed="false" inverse="false" wait="0">
			<parameter type="STATIC" name="Application Name" value="AUC_app" />
		</step>			
		
		<!--########################  INSTALL THE APP  #######################################-->				
		<step name="INSTALL" site="DeVry" page="Home" type="APPLICATION" active="true" threshold="240000" >
			<parameter type="STATIC" name="Application Name" value="AUC_app" />
			<parameter type="STATIC" name="Instrument" value="true" />
		</step>			
		
	</function>
	
	<function name="open_App">
    <description>
		*******************************************************************
		** Function Name: open_App
		** Function Desc: Opens the AUC App
		** Function Params: 
		**	brandApp - the name of the application in the applicationRegistry.xml file to open
		** CODE SAMPLE:
		**		To Call this function, pass the parameters above as follows
		**<!--  
			<step name="open_App" page="Login" type="CALL2" />
		**-->
		** Created By: Andy Dowiarz
		** Change Log: 
		** 			03/20/2017 - adowiarz - New Function
		** NOTES: None 
		*******************************************************************    
	</description>	
		
		<step name="OPEN" site="Ross" page="Home" type="APPLICATION" >
			<parameter type="STATIC" name="Application Name" value="AUC_app" />
		</step>			
		 
	</function>	

	<function name="close_App">
    <description>
		*******************************************************************
		** Function Name: close_App
		** Function Desc: Closes the Devry App
		** Function Params: 
		**	brandApp - the name of the application in the applicationRegistry.xml file to open
		** CODE SAMPLE:
		**		To Call this function, pass the parameters above as follows
		**<!--  
			<step name="close_App" page="Login" type="CALL2" />
		**-->
		** Created By: Andy Dowiarz
		** Change Log: 
		** 			03/20/2017 - adowiarz - New Function
		** NOTES: None 
		*******************************************************************    
	</description>	
			
		<step name="CLOSE" site="Ross" page="Home" type="APPLICATION" >
			<parameter type="STATIC" name="Application Name" value="AUC_app" />
		</step>		
        	
	</function>
			
</suite>